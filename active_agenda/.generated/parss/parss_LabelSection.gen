    $labelSQL = "SELECT 
   `parss`.SharedExpectationScoreID,
   `org2`.Name AS Organization,
   `cod3`.Description AS PolicyTitle,
   `cod2`.Description AS ExpectationGroup,
   `parpe1`.Expectation AS Expectation,
   `org3`.Name AS AuditingOrganization,
   `dat1`.RelatedDate AS ReviewDate,
   `ppl1`.DisplayName AS Auditor,
   `parss`.Score,
   NULLIF(CONCAT(IFNULL(NULLIF(CONCAT(IFNULL(`par1`.PartnershipTitle,'')),''),''),' at ',IFNULL(`org2`.Name,''),' audited by ',IFNULL(`org3`.Name,''),' | ',IFNULL(`parss`.Score,'')),' at  audited by  | ') AS RecordLabel,
   `par1`.OrganizationID AS OrganizationID
FROM `parss`
   
     LEFT OUTER JOIN `para` AS para1 
   ON (`parss`.PartnershipAuditID = `para1`.PartnershipAuditID )
     LEFT OUTER JOIN `parse` AS parse1 
   ON (`parss`.SharedPartnerExpectationID = `parse1`.SharedPartnerExpectationID )
     LEFT OUTER JOIN `dat` AS dat1 
    ON (`parss`.SharedExpectationScoreID = `dat1`.SourceRecordID 
     AND `dat1`.SourceModuleID = 'parss'
     AND `dat1`._Deleted = 0
     AND `dat1`.DateDescriptorID = '9')
     LEFT OUTER JOIN `inv` AS inv1 
    ON (`parss`.SharedExpectationScoreID = `inv1`.SourceRecordID 
     AND `inv1`.SourceModuleID = 'parss'
     AND `inv1`._Deleted = 0
     AND `inv1`.InvolvementDescriptorID = '81')
     LEFT OUTER JOIN `par` AS par1 
   ON (`para1`.PartnershipID = `par1`.PartnershipID )
     LEFT OUTER JOIN `org` AS org3 
   ON (`para1`.AuditingOrganizationID = `org3`.OrganizationID )
     LEFT OUTER JOIN `parpe` AS parpe1 
   ON (`parse1`.PolicyExpectationID = `parpe1`.PolicyExpectationID )
     LEFT OUTER JOIN `ppl` AS ppl1 
   ON (`inv1`.PersonInvolvedID = `ppl1`.PersonID )
     LEFT OUTER JOIN `org` AS org2 
   ON (`par1`.OrganizationID = `org2`.OrganizationID )
     LEFT OUTER JOIN `parp` AS parp2 
   ON (`par1`.PartnershipPolicyID = `parp2`.PartnershipPolicyID )
     LEFT OUTER JOIN `cod` AS cod2 
   ON (`parpe1`.ExpectationGroupID = `cod2`.CodeID 
     AND `cod2`.CodeTypeID = '150')
     LEFT OUTER JOIN `cod` AS cod3 
   ON (`parp2`.PolicyTitleID = `cod3`.CodeID 
     AND `cod3`.CodeTypeID = '201')
WHERE
   parss.SharedExpectationScoreID='/**RecordID**/'";
