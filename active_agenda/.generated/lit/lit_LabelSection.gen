    $singularRecordName = 'Information Technology Loss';
    $labelSQL = "SELECT 
   `lit`.LossITID,
   `clm1`.IncidentReportID AS IncidentReportID,
   `org1`.Name AS Organization,
   NULLIF(CONCAT(IFNULL(`cod3`.Description,''),': ',IFNULL(`litii1`.IndicatorType,'')),': ') AS ITIncidentIndicator,
   `dat5`.RelatedDate AS IncurredTime,
   NULLIF(CONCAT(IFNULL(`cod10`.Description,''),': ',IFNULL(`lstt1`.LossStatus,'')),': ') AS LossStatusType,
   IFNULL(`rskl1`.LikelihoodValue,0) * IFNULL(IF(`csc1`.SeverityValue > `rsks2`.SeverityValue, `csc1`.SeverityValue, `rsks2`.SeverityValue),0) AS IndexValue,
   `bpc1`.IsBestPractice AS IsBestPractice,
   NULLIF(CONCAT(IFNULL(NULLIF(CONCAT(IFNULL(`cod3`.Description,''),': ',IFNULL(`litii1`.IndicatorType,'')),': '),''),' occurring at ',IFNULL(`org1`.Name,''),' on ',IFNULL(`dat5`.RelatedDate,'')),' occurring at  on ') AS RecordLabel,
   `lit`.OrganizationID
FROM `lit`
   
     LEFT OUTER JOIN `clm` AS clm1 
    ON (`lit`.LossITID = `clm1`.RelatedRecordID 
     AND `clm1`.RelatedModuleID = 'lit'
     AND `clm1`._Deleted = 0)
     LEFT OUTER JOIN `org` AS org1 
   ON (`lit`.OrganizationID = `org1`.OrganizationID )
     LEFT OUTER JOIN `litii` AS litii1 
   ON (`lit`.ITIncidentIndicatorID = `litii1`.ITIncidentIndicatorID )
     LEFT OUTER JOIN `rskxa` AS rskxa1 
    ON (`lit`.LossITID = `rskxa1`.SourceRecordID 
     AND `rskxa1`.SourceModuleID = 'lit'
     AND `rskxa1`._Deleted = 0)
     LEFT OUTER JOIN `csc` AS csc1 
    ON (`lit`.LossITID = `csc1`.RecordID 
     AND `csc1`.ModuleID = 'lit'
     AND `csc1`._Deleted = 0)
     LEFT OUTER JOIN (SELECT 
COUNT(`bpc`.BestPracticeID) AS IsBestPractice,
`bpc`.RelatedRecordID FROM `bpc` 
 WHERE `bpc`._Deleted = 0 
 AND 'lit' = `bpc`.RelatedModuleID
GROUP BY `bpc`.RelatedRecordID 
) AS bpc1 
ON (`lit`.LossITID = bpc1.RelatedRecordID) 
     LEFT OUTER JOIN `dat` AS dat5 
    ON (`clm1`.ClaimID = `dat5`.SourceRecordID 
     AND `dat5`.SourceModuleID = 'clm'
     AND `dat5`._Deleted = 0
     AND `dat5`.DateDescriptorID = '34')
     LEFT OUTER JOIN `lstt` AS lstt1 
   ON (`clm1`.LossStatusTypeID = `lstt1`.LossStatusTypeID )
     LEFT OUTER JOIN `cod` AS cod3 
   ON (`litii1`.IndicatorCategoryID = `cod3`.CodeID 
     AND `cod3`.CodeTypeID = '278')
     LEFT OUTER JOIN `rsks` AS rsks2 
   ON (`rskxa1`.SeverityID = `rsks2`.SeverityID )
     LEFT OUTER JOIN `rskl` AS rskl1 
   ON (`rskxa1`.LikelihoodID = `rskl1`.LikelihoodID )
     LEFT OUTER JOIN `cod` AS cod10 
   ON (`lstt1`.LossStatusCategoryID = `cod10`.CodeID 
     AND `cod10`.CodeTypeID = '9')
WHERE
   lit.LossITID='/**RecordID**/'";
