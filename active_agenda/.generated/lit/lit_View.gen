$SQL = "SELECT 
   NULLIF(CONCAT(IFNULL(`org2`.Name,''),' | ',IFNULL(`ins1`.PolicyNumber,'')),' | ') AS Policy,
   `ins1`.IntegrationKey AS IntegrationKey,
   `dat3`.RelatedDate AS ClaimDate,
   `clm1`.ClaimNumber AS ClaimNumber,
   NULLIF(CONCAT(IFNULL(`cod10`.Description,''),': ',IFNULL(`lstt1`.LossStatus,'')),': ') AS LossStatusType,
   `lstt1`.LossStatusDescription AS GeneralLossStatusDescription,
   `clm1`.SpecificLossStatusDescription AS SpecificLossStatusDescription,
   `cod7`.Description AS LegitimacyCategory,
   NULLIF(CONCAT(IFNULL(`llit1`.LegitimacyType,'')),'') AS LossLegitimacyType,
   `llit1`.LegitimacyDesc AS LegitimacyDesc,
   `dat4`.RelatedDate AS ClosureDate,
   `lit`.OrganizationID
FROM `lit`
   
     LEFT OUTER JOIN `clm` AS clm1 
    ON (`lit`.LossITID = `clm1`.RelatedRecordID 
     AND `clm1`.RelatedModuleID = 'lit'
     AND `clm1`._Deleted = 0)
