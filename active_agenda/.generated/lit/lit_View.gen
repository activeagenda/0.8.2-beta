     AND `dat4`.DateDescriptorID = '33')
     LEFT OUTER JOIN `org` AS org2 
   ON (`ins1`.CarrierNameID = `org2`.OrganizationID )
     LEFT OUTER JOIN `cod` AS cod10 
   ON (`lstt1`.LossStatusCategoryID = `cod10`.CodeID 
