    $SQL = "SELECT 
   `lit`.LossITID,
   `clm1`.IncidentReportID AS IncidentReportID,
   NULLIF(CONCAT(IFNULL(`ire1`.IncidentName,'')),'') AS IncidentReport,
   `dat1`.RelatedDate AS EventDate,
   `dat5`.RelatedDate AS IncurredTime,
   `dat2`.RelatedDate AS ReportDate,
   `ppl3`.DisplayName AS PersonReporting,
   `ppl2`.DisplayName AS Supervisor,
   `ppl1`.DisplayName AS PersonAccountable,
   `org1`.Name AS Organization,
   NULLIF(CONCAT_WS(', ',`bui1`.BuildingName,`buir1`.RoomName,`orgl1`.LocationName),'') AS Location,
   `lit`.ITIncidentDesc,
   `cod1`.Description AS ITIncidentStatus,
   `cod2`.Description AS IndicatorCategory,
   NULLIF(CONCAT(IFNULL(`cod3`.Description,''),': ',IFNULL(`litii1`.IndicatorType,'')),': ') AS ITIncidentIndicator,
   `lit`.ITIndicatorDesc,
   `cod4`.Description AS ITIndicatorStatus,
   `cod5`.Description AS Shift,
   NULLIF(CONCAT(IFNULL(`uts1`.UnitName,'')),'') AS DurationUnits,
   `lit`.IncidentDuration,
   `lit`.ITLossDesc,
   `bpc1`.IsBestPractice AS IsBestPractice,
   NULLIF(CONCAT(IFNULL(NULLIF(CONCAT(IFNULL(`cod3`.Description,''),': ',IFNULL(`litii1`.IndicatorType,'')),': '),''),' occurring at ',IFNULL(`org1`.Name,''),' on ',IFNULL(`dat5`.RelatedDate,'')),' occurring at  on ') AS RecordLabel,
   `lit`.OrganizationID
FROM `lit`
   
     LEFT OUTER JOIN `clm` AS clm1 
    ON (`lit`.LossITID = `clm1`.RelatedRecordID 
     AND `clm1`.RelatedModuleID = 'lit'
     AND `clm1`._Deleted = 0)
     LEFT OUTER JOIN `inv` AS inv1 
    ON (`lit`.LossITID = `inv1`.SourceRecordID 
     AND `inv1`.SourceModuleID = 'lit'
     AND `inv1`._Deleted = 0
     AND `inv1`.InvolvementDescriptorID = '55')
     LEFT OUTER JOIN `acc` AS acc1 
    ON (`lit`.LossITID = `acc1`.SourceRecordID 
     AND `acc1`.SourceModuleID = 'lit'
     AND `acc1`._Deleted = 0
     AND `acc1`.AccountabilityDescriptorID = '29')
     LEFT OUTER JOIN `org` AS org1 
   ON (`lit`.OrganizationID = `org1`.OrganizationID )
     LEFT OUTER JOIN `cod` AS cod1 
   ON (`lit`.ITIncidentStatusID = `cod1`.CodeID 
     AND `cod1`.CodeTypeID = '279')
     LEFT OUTER JOIN `litii` AS litii1 
   ON (`lit`.ITIncidentIndicatorID = `litii1`.ITIncidentIndicatorID )
     LEFT OUTER JOIN `cod` AS cod4 
   ON (`lit`.ITIndicatorStatusID = `cod4`.CodeID 
     AND `cod4`.CodeTypeID = '280')
     LEFT OUTER JOIN `cod` AS cod5 
   ON (`lit`.ShiftID = `cod5`.CodeID 
     AND `cod5`.CodeTypeID = '25')
     LEFT OUTER JOIN `uts` AS uts1 
   ON (`lit`.DurationUnitsID = `uts1`.UnitID )
     LEFT OUTER JOIN (SELECT 
COUNT(`bpc`.BestPracticeID) AS IsBestPractice,
`bpc`.RelatedRecordID FROM `bpc` 
 WHERE `bpc`._Deleted = 0 
 AND 'lit' = `bpc`.RelatedModuleID
GROUP BY `bpc`.RelatedRecordID 
) AS bpc1 
ON (`lit`.LossITID = bpc1.RelatedRecordID) 
     LEFT OUTER JOIN `ire` AS ire1 
   ON (`clm1`.IncidentReportID = `ire1`.IncidentReportID )
     LEFT OUTER JOIN `dat` AS dat5 
    ON (`clm1`.ClaimID = `dat5`.SourceRecordID 
     AND `dat5`.SourceModuleID = 'clm'
     AND `dat5`._Deleted = 0
     AND `dat5`.DateDescriptorID = '34')
     LEFT OUTER JOIN `dat` AS dat2 
    ON (`clm1`.ClaimID = `dat2`.SourceRecordID 
     AND `dat2`.SourceModuleID = 'clm'
     AND `dat2`._Deleted = 0
     AND `dat2`.DateDescriptorID = '32')
     LEFT OUTER JOIN `inv` AS inv2 
    ON (`clm1`.ClaimID = `inv2`.SourceRecordID 
     AND `inv2`.SourceModuleID = 'clm'
     AND `inv2`._Deleted = 0
     AND `inv2`.InvolvementDescriptorID = '53')
     LEFT OUTER JOIN `ppl` AS ppl3 
   ON (`inv2`.PersonInvolvedID = `ppl3`.PersonID )
     LEFT OUTER JOIN `orgla` AS orgla1 
    ON (`clm1`.ClaimID = `orgla1`.SourceRecordID 
     AND `orgla1`.SourceModuleID = 'clm'
     AND `orgla1`._Deleted = 0)
     LEFT OUTER JOIN `orgl` AS orgl1 
   ON (`orgla1`.LocationID = `orgl1`.LocationID )
     LEFT OUTER JOIN `ppl` AS ppl2 
   ON (`inv1`.PersonInvolvedID = `ppl2`.PersonID )
     LEFT OUTER JOIN `ppl` AS ppl1 
   ON (`acc1`.PersonAccountableID = `ppl1`.PersonID )
     LEFT OUTER JOIN `cod` AS cod2 
   ON (`litii1`.IndicatorCategoryID = `cod2`.CodeID 
     AND `cod2`.CodeTypeID = '278')
     LEFT OUTER JOIN `cod` AS cod3 
   ON (`litii1`.IndicatorCategoryID = `cod3`.CodeID 
     AND `cod3`.CodeTypeID = '278')
     LEFT OUTER JOIN `dat` AS dat1 
    ON (`ire1`.IncidentReportID = `dat1`.SourceRecordID 
     AND `dat1`.SourceModuleID = 'ire'
     AND `dat1`._Deleted = 0
     AND `dat1`.DateDescriptorID = '16')
     LEFT OUTER JOIN `buir` AS buir1 
   ON (`orgl1`.RoomID = `buir1`.RoomID )
     LEFT OUTER JOIN `bui` AS bui1 
   ON (`buir1`.BuildingID = `bui1`.BuildingID )
WHERE
