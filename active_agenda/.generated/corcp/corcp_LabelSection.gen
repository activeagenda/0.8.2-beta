     LEFT OUTER JOIN `corst` AS corst1 
   ON (`corcp`.CorrectiveSituationTypeID = `corst1`.CorrectiveSituationTypeID )
     LEFT OUTER JOIN `cod` AS cod2 
   ON (`corcp`.OccurNoID = `cod2`.CodeID 
