     LEFT OUTER JOIN `cod` AS cod3 
   ON (`corcp`.CorrectivePracticeCategoryID = `cod3`.CodeID 
     AND `cod3`.CodeTypeID = '57')
     LEFT OUTER JOIN `org` AS org1 
   ON (`corst1`.PolicyOrganizationID = `org1`.OrganizationID )
