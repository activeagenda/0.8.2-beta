    $labelSQL = "SELECT 
   `cosex`.CostExposureID,
   `mod1`.Name AS RelatedModule,
   `rdc1`.Value AS RelatedRecordDescription,
   CONCAT('internal:view.php?mdl=',`cosex`.RelatedModuleID,'&rid=',`cosex`.RelatedRecordID) AS RelatedRecordLink,
   `cosex`.CostExposureTitle,
   `cosex`.Estimate,
   NULLIF(CONCAT(IFNULL(`cosex`.FinancialDetrimentFrequency,''),' per ',IFNULL(NULLIF(CONCAT(IFNULL(`uts1`.UnitName,'')),''),'')),' per ') AS DetrimentFrequency,
   NULLIF(CONCAT(IFNULL(`mod1`.Name,''),' | ',IFNULL(NULLIF(CONCAT(IFNULL(`cost1`.CostTitle,'')),''),''),' | ',IFNULL(`cosex`.CostExposureTitle,'')),' |  | ') AS RecordLabel,
   `cosex`.OrganizationID
FROM `cosex`
   
     LEFT OUTER JOIN `mod` AS mod1 
   ON (`cosex`.RelatedModuleID = `mod1`.ModuleID )
     LEFT OUTER JOIN `rdc` AS rdc1 ON (`cosex`.RelatedRecordID = rdc1.RecordID AND `cosex`.RelatedModuleID = rdc1.ModuleID)
     LEFT OUTER JOIN `uts` AS uts1 
   ON (`cosex`.FinancialDetrimentFrequencyUnitID = `uts1`.UnitID )
     LEFT OUTER JOIN `cost` AS cost1 
   ON (`cosex`.CostTypeID = `cost1`.CostTypeID )
WHERE
   cosex.CostExposureID='/**RecordID**/'";
