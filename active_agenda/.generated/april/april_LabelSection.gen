    $labelSQL = "SELECT 
   `april`.OtherItemLocationID,
   `cod1`.Description AS OtherItemType,
   `aproi1`.OtherItemTitle AS OtherItemTitle,
   `org1`.Name AS Organization,
   NULLIF(CONCAT_WS(', ',`bui1`.BuildingName,`buir1`.RoomName,`orgl1`.LocationName),'') AS Location,
   `bpc1`.IsBestPractice AS IsBestPractice,
   NULLIF(CONCAT(IFNULL(`aproi1`.OtherItemTitle,''),' | ',IFNULL(`org1`.Name,''),' | ',IFNULL(NULLIF(CONCAT_WS(', ',`bui1`.BuildingName,`buir1`.RoomName,`orgl1`.LocationName),''),'')),' |  | ') AS RecordLabel,
   `april`.OrganizationID
FROM `april`
   
     LEFT OUTER JOIN `aproi` AS aproi1 
   ON (`april`.OtherItemID = `aproi1`.OtherItemID )
     LEFT OUTER JOIN `org` AS org1 
   ON (`april`.OrganizationID = `org1`.OrganizationID )
     LEFT OUTER JOIN `orgla` AS orgla1 
    ON (`april`.OtherItemLocationID = `orgla1`.SourceRecordID 
     AND `orgla1`.SourceModuleID = 'april'
     AND `orgla1`._Deleted = 0)
     LEFT OUTER JOIN (SELECT 
COUNT(`bpc`.BestPracticeID) AS IsBestPractice,
`bpc`.RelatedRecordID FROM `bpc` 
 WHERE `bpc`._Deleted = 0 
 AND 'april' = `bpc`.RelatedModuleID
GROUP BY `bpc`.RelatedRecordID 
) AS bpc1 
ON (`april`.OtherItemLocationID = bpc1.RelatedRecordID) 
     LEFT OUTER JOIN `cod` AS cod1 
   ON (`aproi1`.OtherItemTypeID = `cod1`.CodeID 
     AND `cod1`.CodeTypeID = '197')
     LEFT OUTER JOIN `orgl` AS orgl1 
   ON (`orgla1`.LocationID = `orgl1`.LocationID )
     LEFT OUTER JOIN `buir` AS buir1 
   ON (`orgl1`.RoomID = `buir1`.RoomID )
     LEFT OUTER JOIN `bui` AS bui1 
   ON (`buir1`.BuildingID = `bui1`.BuildingID )
WHERE
   april.OtherItemLocationID='/**RecordID**/'";
