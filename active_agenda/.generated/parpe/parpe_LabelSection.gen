   `cod2`.Description AS ExpectationGroup,
   `parpe`.Expectation,
   NULLIF(CONCAT(' Policy Organization: ',IFNULL(`org1`.Name,''),' | ',IFNULL(NULLIF(CONCAT(IFNULL(`cod1`.Description,'')),''),''),' - ',IFNULL(`cod2`.Description,'')),' Policy Organization:  |  - ') AS RecordLabel,
