    $labelSQL = "SELECT 
   `sys`.SystemID,
   `org1`.Name AS Organization,
   NULLIF(CONCAT_WS(', ',`bui1`.BuildingName,`buir1`.RoomName,`orgl1`.LocationName),'') AS Location,
   `cod1`.Description AS SystemCategory,
   NULLIF(CONCAT(IFNULL(`syst1`.SystemTypeName,'')),'') AS SystemType,
   `sys`.SystemName,
   `bpc1`.IsBestPractice AS IsBestPractice,
   NULLIF(CONCAT(IFNULL(`sys`.SystemName,''),' | ',IFNULL(`org1`.Name,''),' | ',IFNULL(NULLIF(CONCAT_WS(', ',`bui1`.BuildingName,`buir1`.RoomName,`orgl1`.LocationName),''),'')),' |  | ') AS RecordLabel,
   `sys`.OrganizationID
FROM `sys`
   
     LEFT OUTER JOIN `org` AS org1 
   ON (`sys`.OrganizationID = `org1`.OrganizationID )
     LEFT OUTER JOIN `orgla` AS orgla1 
    ON (`sys`.SystemID = `orgla1`.SourceRecordID 
     AND `orgla1`.SourceModuleID = 'sys'
     AND `orgla1`._Deleted = 0)
     LEFT OUTER JOIN `syst` AS syst1 
   ON (`sys`.SystemTypeID = `syst1`.SystemTypeID )
     LEFT OUTER JOIN (SELECT 
COUNT(`bpc`.BestPracticeID) AS IsBestPractice,
`bpc`.RelatedRecordID FROM `bpc` 
 WHERE `bpc`._Deleted = 0 
 AND 'sys' = `bpc`.RelatedModuleID
GROUP BY `bpc`.RelatedRecordID 
) AS bpc1 
ON (`sys`.SystemID = bpc1.RelatedRecordID) 
     LEFT OUTER JOIN `orgl` AS orgl1 
   ON (`orgla1`.LocationID = `orgl1`.LocationID )
     LEFT OUTER JOIN `cod` AS cod1 
   ON (`syst1`.SystemCategoryID = `cod1`.CodeID 
     AND `cod1`.CodeTypeID = '82')
     LEFT OUTER JOIN `buir` AS buir1 
   ON (`orgl1`.RoomID = `buir1`.RoomID )
     LEFT OUTER JOIN `bui` AS bui1 
   ON (`buir1`.BuildingID = `bui1`.BuildingID )
WHERE
   sys.SystemID='/**RecordID**/'";
