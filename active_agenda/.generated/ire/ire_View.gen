ORDER BY `rskx`.IndexValue DESC
LIMIT 1) AS RiskRecommendation,
   `ire`.OrganizationID
FROM `ire`
   
     LEFT OUTER JOIN `csc` AS csc1 
    ON (`ire`.IncidentReportID = `csc1`.RecordID 
     AND `csc1`.ModuleID = 'ire'
