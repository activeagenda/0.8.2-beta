    $singularRecordName = 'Vehicle Loss';
    $labelSQL = "SELECT 
   `lppv`.LossVehicleID,
   `org2`.Name AS AssignedOrganization,
   NULLIF(CONCAT(IFNULL(NULLIF(CONCAT(IFNULL(`cod2`.Description,''),' ',IFNULL(`vehmd2`.VehModel,''),' ',IFNULL(`vehmd2`.VehModelNo,'')),'  '),''),' | ',IFNULL(`veh1`.VehLicenseNo,'')),' | ') AS Vehicle,
   `dat5`.RelatedDate AS IncurredTime,
   NULLIF(CONCAT(IFNULL(`cod8`.Description,''),': ',IFNULL(`lstt1`.LossStatus,'')),': ') AS LossStatusType,
   IFNULL(`rskl1`.LikelihoodValue,0) * IFNULL(IF(`csc1`.SeverityValue > `rsks2`.SeverityValue, `csc1`.SeverityValue, `rsks2`.SeverityValue),0) AS IndexValue,
   `bpc1`.IsBestPractice AS IsBestPractice,
   NULLIF(CONCAT(IFNULL(NULLIF(CONCAT(IFNULL(NULLIF(CONCAT(IFNULL(`cod2`.Description,''),' ',IFNULL(`vehmd2`.VehModel,''),' ',IFNULL(`vehmd2`.VehModelNo,'')),'  '),''),' | ',IFNULL(`veh1`.VehLicenseNo,'')),' | '),''),' incurred a loss at ',IFNULL(`org2`.Name,''),' on ',IFNULL(`dat5`.RelatedDate,'')),' incurred a loss at  on ') AS RecordLabel,
   `veh1`.AssignedOrganizationID AS AssignedOrganizationID
FROM `lppv`
   
     LEFT OUTER JOIN `veh` AS veh1 
   ON (`lppv`.VehicleID = `veh1`.VehicleID )
     LEFT OUTER JOIN `clm` AS clm1 
    ON (`lppv`.LossVehicleID = `clm1`.RelatedRecordID 
     AND `clm1`.RelatedModuleID = 'lppv'
     AND `clm1`._Deleted = 0)
     LEFT OUTER JOIN `rskxa` AS rskxa1 
    ON (`lppv`.LossVehicleID = `rskxa1`.SourceRecordID 
     AND `rskxa1`.SourceModuleID = 'lppv'
     AND `rskxa1`._Deleted = 0)
     LEFT OUTER JOIN `csc` AS csc1 
    ON (`lppv`.LossVehicleID = `csc1`.RecordID 
     AND `csc1`.ModuleID = 'lppv'
     AND `csc1`._Deleted = 0)
     LEFT OUTER JOIN (SELECT 
COUNT(`bpc`.BestPracticeID) AS IsBestPractice,
`bpc`.RelatedRecordID FROM `bpc` 
 WHERE `bpc`._Deleted = 0 
 AND 'lppv' = `bpc`.RelatedModuleID
GROUP BY `bpc`.RelatedRecordID 
) AS bpc1 
ON (`lppv`.LossVehicleID = bpc1.RelatedRecordID) 
     LEFT OUTER JOIN `org` AS org2 
   ON (`veh1`.AssignedOrganizationID = `org2`.OrganizationID )
     LEFT OUTER JOIN `vehmd` AS vehmd2 
   ON (`veh1`.VehicleModelID = `vehmd2`.VehicleModelID )
     LEFT OUTER JOIN `dat` AS dat5 
    ON (`clm1`.ClaimID = `dat5`.SourceRecordID 
     AND `dat5`.SourceModuleID = 'clm'
     AND `dat5`._Deleted = 0
     AND `dat5`.DateDescriptorID = '34')
     LEFT OUTER JOIN `lstt` AS lstt1 
   ON (`clm1`.LossStatusTypeID = `lstt1`.LossStatusTypeID )
     LEFT OUTER JOIN `rsks` AS rsks2 
   ON (`rskxa1`.SeverityID = `rsks2`.SeverityID )
     LEFT OUTER JOIN `rskl` AS rskl1 
   ON (`rskxa1`.LikelihoodID = `rskl1`.LikelihoodID )
     LEFT OUTER JOIN `cod` AS cod2 
   ON (`vehmd2`.VehMakeID = `cod2`.CodeID 
     AND `cod2`.CodeTypeID = '29')
     LEFT OUTER JOIN `cod` AS cod8 
   ON (`lstt1`.LossStatusCategoryID = `cod8`.CodeID 
     AND `cod8`.CodeTypeID = '9')
WHERE
   lppv.LossVehicleID='/**RecordID**/'";
