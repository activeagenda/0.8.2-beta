    $labelSQL = "SELECT 
   `iret`.TelephoneCallID,
   `iret`.IncidentReportID,
   `org1`.Name AS PhoningOrganization,
   `dat1`.RelatedDate AS CallDate,
   `iret`.CallTime,
   `org2`.Name AS Organization,
   `bpc1`.IsBestPractice AS IsBestPractice,
   NULLIF(CONCAT(IFNULL(`org1`.Name,''),' called on ',IFNULL(`dat1`.RelatedDate,''),' at ',IFNULL(`iret`.CallTime,'')),' called on  at ') AS RecordLabel,
   `iret`.OrganizationID
FROM `iret`
   
     LEFT OUTER JOIN `org` AS org1 
   ON (`iret`.PhoningOrganizationID = `org1`.OrganizationID )
     LEFT OUTER JOIN `dat` AS dat1 
    ON (`iret`.TelephoneCallID = `dat1`.SourceRecordID 
     AND `dat1`.SourceModuleID = 'iret'
     AND `dat1`._Deleted = 0
     AND `dat1`.DateDescriptorID = '5')
     LEFT OUTER JOIN `org` AS org2 
   ON (`iret`.OrganizationID = `org2`.OrganizationID )
     LEFT OUTER JOIN (SELECT 
COUNT(`bpc`.BestPracticeID) AS IsBestPractice,
`bpc`.RelatedRecordID FROM `bpc` 
 WHERE `bpc`._Deleted = 0 
 AND 'iret' = `bpc`.RelatedModuleID
GROUP BY `bpc`.RelatedRecordID 
) AS bpc1 
ON (`iret`.TelephoneCallID = bpc1.RelatedRecordID) 
WHERE
   iret.TelephoneCallID='/**RecordID**/'";
