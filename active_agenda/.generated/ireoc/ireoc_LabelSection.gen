    $labelSQL = "SELECT 
   `ireoc`.OutCounselID,
   `ireoc`.IncidentReportID,
   `org1`.Name AS Organization,
   `org2`.Name AS LawFirm,
   `cod1`.Description AS LitigationRelationship,
   `cod3`.Description AS ResolutionOutcome,
   `ireoc`.ResolutionCost,
   `bpc1`.IsBestPractice AS IsBestPractice,
   NULLIF(CONCAT(IFNULL(`ire1`.IncidentName,''),' involving ',IFNULL(`org2`.Name,''),' assigned on ',IFNULL(`dat1`.RelatedDate,''),' and currently ',IFNULL(`cod2`.Description,'')),' involving  assigned on  and currently ') AS RecordLabel,
   `ireoc`.OrganizationID
FROM `ireoc`
   
     LEFT OUTER JOIN `org` AS org1 
   ON (`ireoc`.OrganizationID = `org1`.OrganizationID )
     LEFT OUTER JOIN `org` AS org2 
   ON (`ireoc`.LawFirmID = `org2`.OrganizationID )
     LEFT OUTER JOIN `cod` AS cod1 
   ON (`ireoc`.LitigationRelationshipID = `cod1`.CodeID 
     AND `cod1`.CodeTypeID = '184')
     LEFT OUTER JOIN `cod` AS cod3 
   ON (`ireoc`.ResolutionOutcomeID = `cod3`.CodeID 
     AND `cod3`.CodeTypeID = '232')
     LEFT OUTER JOIN (SELECT 
COUNT(`bpc`.BestPracticeID) AS IsBestPractice,
`bpc`.RelatedRecordID FROM `bpc` 
 WHERE `bpc`._Deleted = 0 
 AND 'ireoc' = `bpc`.RelatedModuleID
GROUP BY `bpc`.RelatedRecordID 
) AS bpc1 
ON (`ireoc`.OutCounselID = bpc1.RelatedRecordID) 
     LEFT OUTER JOIN `cod` AS cod2 
   ON (`ireoc`.RepresentationStatusID = `cod2`.CodeID 
     AND `cod2`.CodeTypeID = '231')
     LEFT OUTER JOIN `dat` AS dat1 
    ON (`ireoc`.OutCounselID = `dat1`.SourceRecordID 
     AND `dat1`.SourceModuleID = 'ireoc'
     AND `dat1`._Deleted = 0
     AND `dat1`.DateDescriptorID = '2')
     LEFT OUTER JOIN `ire` AS ire1 
   ON (`ireoc`.IncidentReportID = `ire1`.IncidentReportID )
WHERE
   ireoc.OutCounselID='/**RecordID**/'";
