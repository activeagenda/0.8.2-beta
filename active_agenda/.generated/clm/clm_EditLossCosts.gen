                FROM `clm_l`
                INNER JOIN (
                    SELECT
                        ClaimID,
                        MIN(_RecordID) AS MinRecordID
                    FROM `clm_l`
                    GROUP BY ClaimID) as `clm_min`
                ON `clm_l`._RecordID = `clm_min`.MinRecordID
                ) AS clm_l
                ON (`clm`.ClaimID = clm_l.ClaimID)
     LEFT OUTER JOIN `ppl` AS ppl2 
   ON (`clm_l`.createBy = `ppl2`.PersonID )
     LEFT OUTER JOIN `dat` AS dat4 
    ON (`clm`.ClaimID = `dat4`.SourceRecordID 
     AND `dat4`.SourceModuleID = 'clm'
     AND `dat4`._Deleted = 0
     AND `dat4`.DateDescriptorID = '35')
     LEFT OUTER JOIN `dat` AS dat5 
    ON (`clm`.ClaimID = `dat5`.SourceRecordID 
     AND `dat5`.SourceModuleID = 'clm'
     AND `dat5`._Deleted = 0
     AND `dat5`.DateDescriptorID = '33')
     LEFT OUTER JOIN `inv` AS inv1 
