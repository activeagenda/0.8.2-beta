    $labelSQL = "SELECT 
   `clm`.ClaimID,
   `clm`.IncidentReportID,
   `mod1`.Name AS RelatedModule,
   `rdc1`.Value AS RelatedRecordDescription,
   CONCAT('internal:view.php?mdl=',`clm`.RelatedModuleID,'&rid=',`clm`.RelatedRecordID) AS RelatedRecordLink,
   `dat4`.RelatedDate AS ClaimDate,
   `lcod1`.TotalIncurred AS TotalIncurred,
   NULLIF(CONCAT(IFNULL(`mod1`.Name,''),' | ',IFNULL(`org1`.Name,''),' | ',IFNULL(`dat1`.RelatedDate,'')),' |  | ') AS RecordLabel,
   `rdc1`.OrganizationID AS OwnerOrganizationID
FROM `clm`
   
     LEFT OUTER JOIN `mod` AS mod1 
   ON (`clm`.RelatedModuleID = `mod1`.ModuleID )
     LEFT OUTER JOIN `rdc` AS rdc1 ON (`clm`.RelatedRecordID = rdc1.RecordID AND `clm`.RelatedModuleID = rdc1.ModuleID)
     LEFT OUTER JOIN `dat` AS dat4 
    ON (`clm`.ClaimID = `dat4`.SourceRecordID 
     AND `dat4`.SourceModuleID = 'clm'
     AND `dat4`._Deleted = 0
     AND `dat4`.DateDescriptorID = '35')
     LEFT OUTER JOIN (SELECT
   `lco_sub1`.LossCostID AS LatestLossCostID,
   `lco_sub1`.ClaimID
FROM 
   `lco` AS `lco_sub1`
WHERE
    `lco_sub1`._Deleted = 0
   AND `lco_sub1`.LossCostID = (
      select `lco_sub2`.LossCostID
      from `lco` AS `lco_sub2`
      where `lco_sub2`._Deleted = 0
        and `lco_sub2`.ClaimID = `lco_sub1`.ClaimID
      order by `lco_sub2`.ValuationDate DESC
      limit 1
   )) AS `lco1` ON 
            `clm`.ClaimID = lco1.ClaimID

     LEFT OUTER JOIN `ire` AS ire1 
   ON (`clm`.IncidentReportID = `ire1`.IncidentReportID )
     LEFT OUTER JOIN `org` AS org1 
   ON (`rdc1`.OrganizationID = `org1`.OrganizationID )
     LEFT OUTER JOIN (SELECT 
SUM(ROUND(`lcod`.Incurred, 2)) AS TotalIncurred,
`lcod`.LossCostID FROM `lcod` 
 WHERE `lcod`._Deleted = 0 
GROUP BY `lcod`.LossCostID 
) AS lcod1 
ON (`lco1`.LatestLossCostID = lcod1.LossCostID) 
     LEFT OUTER JOIN `dat` AS dat1 
    ON (`ire1`.IncidentReportID = `dat1`.SourceRecordID 
     AND `dat1`.SourceModuleID = 'ire'
     AND `dat1`._Deleted = 0
     AND `dat1`.DateDescriptorID = '16')
WHERE
   clm.ClaimID='/**RecordID**/'";
