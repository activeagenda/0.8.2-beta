    $labelSQL = "SELECT 
   `twn`.TownHallQuestionID,
   `org1`.Name AS Organization,
   `ppl1`.DisplayName AS SubmittedBy,
   `ppl2`.DisplayName AS PersonAsked,
   `twn`.QuestionAsked,
   `cod2`.Description AS QuestionStatus,
   `bpc1`.IsBestPractice AS IsBestPractice,
   NULLIF(CONCAT(IFNULL(`ppl1`.DisplayName,''),' asking: ',IFNULL(`twn`.QuestionAsked,'')),' asking: ') AS RecordLabel,
   `twn`.OrganizationID
FROM `twn`
   
     LEFT OUTER JOIN `org` AS org1 
   ON (`twn`.OrganizationID = `org1`.OrganizationID )
     LEFT OUTER JOIN `inv` AS inv1 
    ON (`twn`.TownHallQuestionID = `inv1`.SourceRecordID 
     AND `inv1`.SourceModuleID = 'twn'
     AND `inv1`._Deleted = 0
     AND `inv1`.InvolvementDescriptorID = '43')
     LEFT OUTER JOIN `inv` AS inv2 
    ON (`twn`.TownHallQuestionID = `inv2`.SourceRecordID 
     AND `inv2`.SourceModuleID = 'twn'
     AND `inv2`._Deleted = 0
     AND `inv2`.InvolvementDescriptorID = '68')
     LEFT OUTER JOIN `cod` AS cod2 
   ON (`twn`.QuestionStatusID = `cod2`.CodeID 
     AND `cod2`.CodeTypeID = '221')
     LEFT OUTER JOIN (SELECT 
COUNT(`bpc`.BestPracticeID) AS IsBestPractice,
`bpc`.RelatedRecordID FROM `bpc` 
 WHERE `bpc`._Deleted = 0 
 AND 'twn' = `bpc`.RelatedModuleID
GROUP BY `bpc`.RelatedRecordID 
) AS bpc1 
ON (`twn`.TownHallQuestionID = bpc1.RelatedRecordID) 
     LEFT OUTER JOIN `ppl` AS ppl1 
   ON (`inv1`.PersonInvolvedID = `ppl1`.PersonID )
     LEFT OUTER JOIN `ppl` AS ppl2 
   ON (`inv2`.PersonInvolvedID = `ppl2`.PersonID )
WHERE
   twn.TownHallQuestionID='/**RecordID**/'";
