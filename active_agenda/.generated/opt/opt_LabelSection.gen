    $labelSQL = "SELECT 
   `opt`.OpportunityEstimateID,
   `mod1`.Name AS RelatedModule,
   `rdc1`.Value AS RelatedRecordDescription,
   CONCAT('internal:view.php?mdl=',`opt`.RelatedModuleID,'&rid=',`opt`.RelatedRecordID) AS RelatedRecordLink,
   `opt`.OpportunityEstimateTitle,
   `opt`.Estimate,
   NULLIF(CONCAT(IFNULL(`opt`.FinancialBenefitFrequency,''),' per ',IFNULL(NULLIF(CONCAT(IFNULL(`uts1`.UnitName,'')),''),'')),' per ') AS BenefitFrequency,
   NULLIF(CONCAT(IFNULL(`mod1`.Name,''),' | ',IFNULL(`opt`.OpportunityEstimateTitle,''),' | ',IFNULL(ROUND(`opt`.Estimate, 2),''),' | ',IFNULL(NULLIF(CONCAT(IFNULL(`opt`.FinancialBenefitFrequency,''),' per ',IFNULL(NULLIF(CONCAT(IFNULL(`uts1`.UnitName,'')),''),'')),' per '),'')),' |  |  | ') AS RecordLabel,
   `opt`.OwnerOrganizationID
FROM `opt`
   
     LEFT OUTER JOIN `mod` AS mod1 
   ON (`opt`.RelatedModuleID = `mod1`.ModuleID )
     LEFT OUTER JOIN `rdc` AS rdc1 ON (`opt`.RelatedRecordID = rdc1.RecordID AND `opt`.RelatedModuleID = rdc1.ModuleID)
     LEFT OUTER JOIN `uts` AS uts1 
   ON (`opt`.FinancialBenefitFrequencyUnitID = `uts1`.UnitID )
WHERE
