    $labelSQL = "SELECT 
   `oppap`.PermitAppID,
   NULLIF(CONCAT(IFNULL(`opp1`.PermitName,'')),'') AS OppPermit,
   `cod1`.Description AS AppType,
   `dat1`.RelatedDate AS AppDate,
   `oppap`.AppAppr,
   `oppap`.AppExp,
   `dat1`.RelatedDate AS ReAppDate,
   NULLIF(CONCAT(IFNULL(NULLIF(CONCAT(IFNULL(`opp1`.PermitName,'')),''),''),' | ',IFNULL(`cod1`.Description,''),' | ',IFNULL(`dat1`.RelatedDate,''),' | ',IFNULL(`org1`.Name,'')),' |  |  | ') AS RecordLabel,
   `opp1`.OrganizationID AS OrganizationID
FROM `oppap`
   
     LEFT OUTER JOIN `opp` AS opp1 
   ON (`oppap`.OppPermitID = `opp1`.OppPermitID )
     LEFT OUTER JOIN `cod` AS cod1 
   ON (`oppap`.AppTypeID = `cod1`.CodeID 
     AND `cod1`.CodeTypeID = '100')
     LEFT OUTER JOIN `dat` AS dat1 
    ON (`oppap`.PermitAppID = `dat1`.SourceRecordID 
     AND `dat1`.SourceModuleID = 'oppap'
     AND `dat1`._Deleted = 0
     AND `dat1`.DateDescriptorID = '39')
     LEFT OUTER JOIN `org` AS org1 
   ON (`opp1`.OrganizationID = `org1`.OrganizationID )
WHERE
   oppap.PermitAppID='/**RecordID**/'";
