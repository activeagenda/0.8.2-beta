     LEFT OUTER JOIN `mod` AS mod1 
   ON (`orgda`.RelatedModuleID = `mod1`.ModuleID )
     LEFT OUTER JOIN `rdc` AS rdc1 ON (`orgda`.RelatedRecordID = rdc1.RecordID AND `orgda`.RelatedModuleID = rdc1.ModuleID)
