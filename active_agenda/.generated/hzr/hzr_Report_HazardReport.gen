     AND `orgla1`.SourceModuleID = \'hza\'
     AND `orgla1`._Deleted = 0)
LEFT OUTER JOIN `acc` AS acc1 
    ON (`hza`.HazardID = `acc1`.SourceRecordID 
     AND `acc1`.SourceModuleID = \'hza\'
     AND `acc1`._Deleted = 0
     AND `acc1`.AccountabilityDescriptorID = \'35\')
