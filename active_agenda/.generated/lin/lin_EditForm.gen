    /**CUSTOM_CODE|init**/

    $getSQL = "SELECT 
   `lin`.LossInjuryID,
   `clm1`.IncidentReportID AS IncidentReportID,
   NULLIF(CONCAT(IFNULL(`ire1`.IncidentName,'')),'') AS IncidentReport,
   `dat1`.RelatedDate AS EventDate,
   `clm1`.TimeDeterminable AS TimeDeterminable,
   `dat3`.RelatedDate AS IncurredTime,
   `dat6`.RelatedDate AS ReportDate,
   `clm_l`.createDate AS EntryDate,
   `ppl2`.DisplayName AS InjuredPerson,
   `inv1`.PersonInvolvedID AS InjuredPersonID,
   `orgjt1`.JobTitleName AS JobTitle,
   `lin`.JobTitleID,
   `cod3`.Description AS Shift,
   `lin`.ShiftID,
   `lin`.WorkBegan,
   `ppl7`.DisplayName AS SubmittedBy,
   `clm_l`.createBy AS SubmittedByID,
   `ppl1`.DisplayName AS PersonAccountable,
   `acc1`.PersonAccountableID AS PersonAccountableID,
   `ppl3`.DisplayName AS Supervisor,
   `inv2`.PersonInvolvedID AS SupervisorID,
   NULLIF(CONCAT(IFNULL(`ppl2`.DisplayName,''),' of ',IFNULL(`org1`.Name,''),' was injured  on ',IFNULL(`dat3`.RelatedDate,'')),' of  was injured  on ') AS RecordLabel,
   `lin`.OrganizationID
FROM `lin`
   
     LEFT OUTER JOIN `clm` AS clm1 
    ON (`lin`.LossInjuryID = `clm1`.RelatedRecordID 
     AND `clm1`.RelatedModuleID = 'lin'
     AND `clm1`._Deleted = 0)
     LEFT OUTER JOIN `inv` AS inv1 
    ON (`lin`.LossInjuryID = `inv1`.SourceRecordID 
     AND `inv1`.SourceModuleID = 'lin'
     AND `inv1`._Deleted = 0
     AND `inv1`.InvolvementDescriptorID = '44')
     LEFT OUTER JOIN `orgjt` AS orgjt1 
   ON (`lin`.JobTitleID = `orgjt1`.OrgJobTitleID )
     LEFT OUTER JOIN `cod` AS cod3 
   ON (`lin`.ShiftID = `cod3`.CodeID 
     AND `cod3`.CodeTypeID = '25')
     LEFT OUTER JOIN `acc` AS acc1 
    ON (`lin`.LossInjuryID = `acc1`.SourceRecordID 
     AND `acc1`.SourceModuleID = 'lin'
     AND `acc1`._Deleted = 0
     AND `acc1`.AccountabilityDescriptorID = '29')
     LEFT OUTER JOIN `inv` AS inv2 
    ON (`lin`.LossInjuryID = `inv2`.SourceRecordID 
     AND `inv2`.SourceModuleID = 'lin'
     AND `inv2`._Deleted = 0
     AND `inv2`.InvolvementDescriptorID = '55')
     LEFT OUTER JOIN `org` AS org1 
   ON (`lin`.OrganizationID = `org1`.OrganizationID )
     LEFT OUTER JOIN `ire` AS ire1 
   ON (`clm1`.IncidentReportID = `ire1`.IncidentReportID )
     LEFT OUTER JOIN `dat` AS dat3 
    ON (`clm1`.ClaimID = `dat3`.SourceRecordID 
     AND `dat3`.SourceModuleID = 'clm'
     AND `dat3`._Deleted = 0
     AND `dat3`.DateDescriptorID = '34')
     LEFT OUTER JOIN `dat` AS dat6 
    ON (`clm1`.ClaimID = `dat6`.SourceRecordID 
     AND `dat6`.SourceModuleID = 'clm'
     AND `dat6`._Deleted = 0
     AND `dat6`.DateDescriptorID = '32')
     LEFT OUTER JOIN (
                SELECT
                    `clm_l`.ClaimID,
                    `clm_l`._ModDate AS createDate,
                    `clm_l`._ModBy AS createBy
                FROM `clm_l`
                INNER JOIN (
                    SELECT
                        ClaimID,
                        MIN(_RecordID) AS MinRecordID
                    FROM `clm_l`
                    GROUP BY ClaimID) as `clm_min`
                ON `clm_l`._RecordID = `clm_min`.MinRecordID
                ) AS clm_l
                ON (`clm1`.ClaimID = clm_l.ClaimID)
     LEFT OUTER JOIN `ppl` AS ppl7 
   ON (`clm_l`.createBy = `ppl7`.PersonID )
     LEFT OUTER JOIN `ppl` AS ppl2 
   ON (`inv1`.PersonInvolvedID = `ppl2`.PersonID )
     LEFT OUTER JOIN `ppl` AS ppl1 
   ON (`acc1`.PersonAccountableID = `ppl1`.PersonID )
     LEFT OUTER JOIN `ppl` AS ppl3 
   ON (`inv2`.PersonInvolvedID = `ppl3`.PersonID )
     LEFT OUTER JOIN `dat` AS dat1 
    ON (`ire1`.IncidentReportID = `dat1`.SourceRecordID 
     AND `dat1`.SourceModuleID = 'ire'
     AND `dat1`._Deleted = 0
     AND `dat1`.DateDescriptorID = '16')
