    $labelSQL = "SELECT 
   `cat`.CatalogID,
   `cod1`.Description AS CatalogType,
   `cat`.CatalogTitle,
   `org2`.Name AS Organization,
   `cod2`.Description AS CatalogStatus,
   `bpc1`.IsBestPractice AS IsBestPractice,
   NULLIF(CONCAT(IFNULL(`cat`.CatalogTitle,''),' | ',IFNULL(`org1`.Name,''),' | ',IFNULL(`dat1`.RelatedDate,'')),' |  | ') AS RecordLabel,
   `cat`.OrganizationID
FROM `cat`
   
     LEFT OUTER JOIN `cod` AS cod1 
   ON (`cat`.CatalogTypeID = `cod1`.CodeID 
     AND `cod1`.CodeTypeID = '55')
     LEFT OUTER JOIN `org` AS org2 
   ON (`cat`.OrganizationID = `org2`.OrganizationID )
     LEFT OUTER JOIN `cod` AS cod2 
   ON (`cat`.CatalogStatusID = `cod2`.CodeID 
     AND `cod2`.CodeTypeID = '111')
     LEFT OUTER JOIN (SELECT 
COUNT(`bpc`.BestPracticeID) AS IsBestPractice,
`bpc`.RelatedRecordID FROM `bpc` 
 WHERE `bpc`._Deleted = 0 
 AND 'cat' = `bpc`.RelatedModuleID
GROUP BY `bpc`.RelatedRecordID 
) AS bpc1 
ON (`cat`.CatalogID = bpc1.RelatedRecordID) 
     LEFT OUTER JOIN `dat` AS dat1 
    ON (`cat`.CatalogID = `dat1`.SourceRecordID 
     AND `dat1`.SourceModuleID = 'cat'
     AND `dat1`._Deleted = 0
     AND `dat1`.DateDescriptorID = '11')
     LEFT OUTER JOIN `org` AS org1 
   ON (`cat`.CatalogCompanyID = `org1`.OrganizationID )
WHERE
   cat.CatalogID='/**RecordID**/'";
