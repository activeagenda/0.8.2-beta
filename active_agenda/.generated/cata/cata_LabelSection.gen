    $labelSQL = "SELECT 
   `cata`.CatalogAssocID,
   NULLIF(CONCAT(IFNULL(`cat1`.CatalogTitle,'')),'') AS Catalog,
   `org1`.Name AS Organization,
   `cod1`.Description AS Approval,
   `bpc1`.IsBestPractice AS IsBestPractice,
   NULLIF(CONCAT(IFNULL(NULLIF(CONCAT(IFNULL(`cat1`.CatalogTitle,'')),''),''),' | ',IFNULL(`org1`.Name,''),' | ',IFNULL(`cod1`.Description,'')),' |  | ') AS RecordLabel,
   `cata`.OrganizationID
FROM `cata`
   
     LEFT OUTER JOIN `cat` AS cat1 
   ON (`cata`.CatalogID = `cat1`.CatalogID )
     LEFT OUTER JOIN `org` AS org1 
   ON (`cata`.OrganizationID = `org1`.OrganizationID )
     LEFT OUTER JOIN `cod` AS cod1 
   ON (`cata`.ApprovalID = `cod1`.CodeID 
     AND `cod1`.CodeTypeID = '99')
     LEFT OUTER JOIN (SELECT 
COUNT(`bpc`.BestPracticeID) AS IsBestPractice,
`bpc`.RelatedRecordID FROM `bpc` 
 WHERE `bpc`._Deleted = 0 
 AND 'cata' = `bpc`.RelatedModuleID
GROUP BY `bpc`.RelatedRecordID 
) AS bpc1 
ON (`cata`.CatalogAssocID = bpc1.RelatedRecordID) 
WHERE
