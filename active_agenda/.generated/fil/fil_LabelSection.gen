    $singularRecordName = 'File Requirement';
    $labelSQL = "SELECT 
   `fil`.FileReqID,
   `org1`.Name AS Organization,
   `cod1`.Description AS FileCategory,
   NULLIF(CONCAT(IFNULL(`filt1`.FileTypeTitle,'')),'') AS FileType,
   `cod2`.Description AS FileRetentionMethod,
   `cod4`.Description AS FileDispositionMethod,
   `bpc1`.IsBestPractice AS IsBestPractice,
   NULLIF(CONCAT(IFNULL(`org1`.Name,''),' | ',IFNULL(`cod2`.Description,'')),' | ') AS RecordLabel,
   `fil`.OrganizationID
FROM `fil`
   
     LEFT OUTER JOIN `org` AS org1 
   ON (`fil`.OrganizationID = `org1`.OrganizationID )
     LEFT OUTER JOIN `filt` AS filt1 
   ON (`fil`.FileTypeID = `filt1`.FileTypeID )
     LEFT OUTER JOIN `cod` AS cod2 
   ON (`fil`.FileRetentionMethodID = `cod2`.CodeID 
     AND `cod2`.CodeTypeID = '63')
     LEFT OUTER JOIN `cod` AS cod4 
   ON (`fil`.FileDispositionMethodID = `cod4`.CodeID 
     AND `cod4`.CodeTypeID = '79')
     LEFT OUTER JOIN (SELECT 
COUNT(`bpc`.BestPracticeID) AS IsBestPractice,
`bpc`.RelatedRecordID FROM `bpc` 
 WHERE `bpc`._Deleted = 0 
 AND 'fil' = `bpc`.RelatedModuleID
GROUP BY `bpc`.RelatedRecordID 
) AS bpc1 
ON (`fil`.FileReqID = bpc1.RelatedRecordID) 
     LEFT OUTER JOIN `cod` AS cod1 
   ON (`filt1`.FileCategoryID = `cod1`.CodeID 
     AND `cod1`.CodeTypeID = '266')
WHERE
   fil.FileReqID='/**RecordID**/'";
