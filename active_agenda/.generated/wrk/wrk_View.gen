     AND `acc1`.SourceModuleID = 'wrk'
     AND `acc1`._Deleted = 0
     AND `acc1`.AccountabilityDescriptorID = '96')
     LEFT OUTER JOIN `cod` AS cod1 
   ON (`wrk`.WorkOrderTypeID = `cod1`.CodeID 
