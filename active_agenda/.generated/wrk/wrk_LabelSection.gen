    $labelSQL = "SELECT 
   `wrk`.WorkOrderID,
   `mod1`.Name AS RelatedModule,
   `rdc1`.Value AS RelatedRecordDescription,
   CONCAT('internal:view.php?mdl=',`wrk`.RelatedModuleID,'&rid=',`wrk`.RelatedRecordID) AS RelatedRecordLink,
   `wrk`.WorkOrderTitle,
   NULLIF(CONCAT('WO: ',IFNULL(`wrk`.OrganizationID,''),' - ',IFNULL(`wrk`.WorkOrderID,'')),'WO:  - ') AS SystemWorkOrderNumber,
   `dat1`.RelatedDate AS IssuedDate,
   `cod3`.Description AS WorkOrderStatus,
   `bpc1`.IsBestPractice AS IsBestPractice,
   NULLIF(CONCAT(IFNULL(`org1`.Name,''),' | ',IFNULL(`wrk`.WorkOrderTitle,''),' (',IFNULL(NULLIF(CONCAT('WO: ',IFNULL(`wrk`.OrganizationID,''),' - ',IFNULL(`wrk`.WorkOrderID,'')),'WO:  - '),''),') | ',IFNULL(`mod1`.Name,''),' | ',IFNULL(`rdc1`.Value,'')),' |  () |  | ') AS RecordLabel,
   `wrk`.OrganizationID
FROM `wrk`
   
     LEFT OUTER JOIN `mod` AS mod1 
   ON (`wrk`.RelatedModuleID = `mod1`.ModuleID )
     LEFT OUTER JOIN `rdc` AS rdc1 ON (`wrk`.RelatedRecordID = rdc1.RecordID AND `wrk`.RelatedModuleID = rdc1.ModuleID)
     LEFT OUTER JOIN `dat` AS dat1 
    ON (`wrk`.WorkOrderID = `dat1`.SourceRecordID 
     AND `dat1`.SourceModuleID = 'wrk'
     AND `dat1`._Deleted = 0
     AND `dat1`.DateDescriptorID = '47')
     LEFT OUTER JOIN `cod` AS cod3 
   ON (`wrk`.WorkOrderStatusID = `cod3`.CodeID 
     AND `cod3`.CodeTypeID = '258')
     LEFT OUTER JOIN (SELECT 
COUNT(`bpc`.BestPracticeID) AS IsBestPractice,
`bpc`.RelatedRecordID FROM `bpc` 
 WHERE `bpc`._Deleted = 0 
 AND 'wrk' = `bpc`.RelatedModuleID
GROUP BY `bpc`.RelatedRecordID 
) AS bpc1 
ON (`wrk`.WorkOrderID = bpc1.RelatedRecordID) 
     LEFT OUTER JOIN `org` AS org1 
   ON (`wrk`.OrganizationID = `org1`.OrganizationID )
WHERE
   wrk.WorkOrderID='/**RecordID**/'";
