    $singularRecordName = 'Building Loss';
    $labelSQL = "SELECT 
   `lppb`.LossBuildingID,
   `org1`.Name AS Organization,
   `cod2`.Description AS PropOwnership,
   `cod3`.Description AS PropertyLossType,
   `dat5`.RelatedDate AS IncurredTime,
   NULLIF(CONCAT(IFNULL(`cod7`.Description,''),': ',IFNULL(`lstt1`.LossStatus,'')),': ') AS LossStatusType,
   IFNULL(`rskl1`.LikelihoodValue,0) * IFNULL(IF(`csc1`.SeverityValue > `rsks2`.SeverityValue, `csc1`.SeverityValue, `rsks2`.SeverityValue),0) AS IndexValue,
   `bpc1`.IsBestPractice AS IsBestPractice,
   NULLIF(CONCAT(IFNULL(NULLIF(CONCAT(IFNULL(`bui1`.BuildingName,'')),''),''),' of ',IFNULL(`org1`.Name,''),' incurred a loss on ',IFNULL(`dat5`.RelatedDate,'')),' of  incurred a loss on ') AS RecordLabel,
   `lppb`.OrganizationID
FROM `lppb`
   
     LEFT OUTER JOIN `org` AS org1 
   ON (`lppb`.OrganizationID = `org1`.OrganizationID )
     LEFT OUTER JOIN `cod` AS cod2 
   ON (`lppb`.PropOwnershipID = `cod2`.CodeID 
     AND `cod2`.CodeTypeID = '24')
     LEFT OUTER JOIN `cod` AS cod3 
   ON (`lppb`.PropertyLossTypeID = `cod3`.CodeID 
     AND `cod3`.CodeTypeID = '40')
     LEFT OUTER JOIN `clm` AS clm1 
    ON (`lppb`.LossBuildingID = `clm1`.RelatedRecordID 
     AND `clm1`.RelatedModuleID = 'lppb'
     AND `clm1`._Deleted = 0)
     LEFT OUTER JOIN `rskxa` AS rskxa1 
    ON (`lppb`.LossBuildingID = `rskxa1`.SourceRecordID 
     AND `rskxa1`.SourceModuleID = 'lppb'
     AND `rskxa1`._Deleted = 0)
     LEFT OUTER JOIN `csc` AS csc1 
    ON (`lppb`.LossBuildingID = `csc1`.RecordID 
     AND `csc1`.ModuleID = 'lppb'
     AND `csc1`._Deleted = 0)
     LEFT OUTER JOIN (SELECT 
COUNT(`bpc`.BestPracticeID) AS IsBestPractice,
`bpc`.RelatedRecordID FROM `bpc` 
 WHERE `bpc`._Deleted = 0 
 AND 'lppb' = `bpc`.RelatedModuleID
GROUP BY `bpc`.RelatedRecordID 
) AS bpc1 
ON (`lppb`.LossBuildingID = bpc1.RelatedRecordID) 
     LEFT OUTER JOIN `bui` AS bui1 
   ON (`lppb`.BuildingID = `bui1`.BuildingID )
     LEFT OUTER JOIN `dat` AS dat5 
    ON (`clm1`.ClaimID = `dat5`.SourceRecordID 
     AND `dat5`.SourceModuleID = 'clm'
     AND `dat5`._Deleted = 0
     AND `dat5`.DateDescriptorID = '34')
     LEFT OUTER JOIN `lstt` AS lstt1 
   ON (`clm1`.LossStatusTypeID = `lstt1`.LossStatusTypeID )
     LEFT OUTER JOIN `rsks` AS rsks2 
   ON (`rskxa1`.SeverityID = `rsks2`.SeverityID )
     LEFT OUTER JOIN `rskl` AS rskl1 
   ON (`rskxa1`.LikelihoodID = `rskl1`.LikelihoodID )
     LEFT OUTER JOIN `cod` AS cod7 
   ON (`lstt1`.LossStatusCategoryID = `cod7`.CodeID 
     AND `cod7`.CodeTypeID = '9')
WHERE
   lppb.LossBuildingID='/**RecordID**/'";
