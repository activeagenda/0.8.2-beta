		<ForeignField name="SecondPersonAccountable" foreignTable="ppl" foreignKey="PersonID" foreignField="DisplayName" key="SecondPersonAccountableID" joinType="left" phrase="Second Party Accountable|The name of the second person with accountability for the agreement" sample="Mattias Thorslund"/>
		<TableField name="AgreementTypeID" type="int" dbFlags="unsigned not null" phrase="Agreement Type|A combined field used to describe this agreement type when associating it with other records" sample="1"/>
		<ForeignField name="AgreementCategoryID" foreignTable="agrt" foreignKey="AgreementTypeID" foreignField="AgreementCategoryID" key="AgreementTypeID" joinType="left" phrase="Category|A broad categoy used to group agreements of similar types" sample="1"/>
		<CodeField name="AgreementCategory" key="AgreementCategoryID" phrase="Category|A broad categoy used to group agreements of similar types" sample="Employment" codeTypeID="125"/>
		<ForeignField name="AgreementTitleGuide" foreignTable="agrt" foreignKey="AgreementTypeID" foreignField="AgreementTypeTitle" key="AgreementTypeID" joinType="left" phrase="Guidance Type|A title or phrase used to describe a agreement type" sample="Last Chance Agreement"/>
		<ForeignField name="AgreementDescGuide" foreignTable="agrt" foreignKey="AgreementTypeID" foreignField="AgreementTypeDesc" key="AgreementTypeID" joinType="left" phrase="Guidance Description|A brief but thorough description of the agreement type" sample="An agreement between an employee and an employer that establishes criteria for an employee to retain employee. This type of agreement is typically the last step in the behavior correction process."/>
		<ForeignField name="AgreementType" foreignTable="agrt" foreignKey="AgreementTypeID" foreignField="AgreementType" key="AgreementTypeID" joinType="left" phrase="Agreement Type|A combined field used to describe this agreement type when associating it with other records" sample="Employment | Last Chance Agreement"/>
		<TableField name="AgmtTitle" type="varchar(128)" validate="notEmpty" phrase="Agreement Title|A brief phrase used to describe the title of this contract or agreement" sample="Thorslund Last Chance Agreement"/>
		<TableField name="AgmtSummary" type="text" validate="noValidation" phrase="Agreement Summary|A brief summary of the scope and purpose of the business continuation contract or agreement" sample="Mattias has promised to do something to prevent termination."/>
		<TableField name="TerminationFactors" type="text" validate="noValidation" phrase="Termination Factors|A brief summary of any factors which may lead to the termination of the agreement" sample="Failure to maintain normal maintenance will terminate the obligations of the parties."/>
		<RemoteField name="AgreementDate" type="date" validate="RequireSelection" remoteModuleID="dat" remoteModuleIDField="SourceModuleID" remoteRecordIDField="SourceRecordID" remoteField="RelatedDate" remoteDescriptorField="DateDescriptorID" remoteDescriptor="17" phrase="Effective Date|Date the agreement or contract was entered into and became effective" sample="01/01/2003"/>
		<RemoteField name="ExpirationDate" type="date" validate="RequireSelection" remoteModuleID="dat" remoteModuleIDField="SourceModuleID" remoteRecordIDField="SourceRecordID" remoteField="RelatedDate" remoteDescriptorField="DateDescriptorID" remoteDescriptor="10" phrase="Expiration Date|Date the agreement or contract expires" sample="01/01/2004"/>
		<TableField name="AgmtStatusID" type="int" validate="RequireSelection" phrase="Status|Current status of the agreement or contract" sample="1"/>
		<CodeField name="AgmtStatus" key="AgmtStatusID" phrase="Status|Current status of the agreement or contract" sample="Expired" codeTypeID="86"/>
		<TableField name="AgmtAttached" type="bool" validate="noValidation" dbFlags="default 0" phrase="Agreement Attached|Whether a digital copy of the agreement or contract has been attached to this record" sample="Yes"/>
		<SummaryField name="TotalEstExp" type="money" validate="noValidation" summaryFunction="sum" summaryField="Estimate" localKey="AgreementID" summaryKey="RelatedRecordID" summaryModuleID="cosex" summaryCondition="RelatedModuleID = 'agr'" isGlobal="no" phrase="Estimated Exposure|The total estimated cost exposure associated with the agreement" sample="702.00"/>
		<SummaryField name="TotalEstOpp" type="money" validate="noValidation" summaryFunction="sum" summaryField="Estimate" localKey="AgreementID" summaryKey="RelatedRecordID" summaryModuleID="opt" summaryCondition="RelatedModuleID = 'agr'" isGlobal="no" phrase="Estimated Opportunity|The total estimated financial opportunity associated with the agreement" sample="702.00"/>
        <LinkField name="RelatedRecordLink" moduleIDField="RelatedModuleID" recordIDField="RelatedRecordID" phrase="Related Record Link|An internal link to the related record."/>
		<CombinedField name="RelatedModuleRecord" phrase="Related Record|A combined field used to display the related module name and record ID" sample="Corrective Actions | Record ID: 3"><CombinedFieldRef name="RelatedModule"/>_Module_(Record_ID:_<CombinedFieldRef name="RelatedRecordID"/>)</CombinedField>
		<CombinedField name="RecordLabel" phrase="Record Label|The short, concise, label of the record for display purposes" sample="Active Agenda, HQ: Warehouse"><CombinedFieldRef name="Organization"/>;_<CombinedFieldRef name="AgmtTitle"/>;_<CombinedFieldRef name="RelatedModule"/>;_<CombinedFieldRef name="RelatedRecordDescription"/></CombinedField>
		<CombinedField name="Agreement" phrase="Agreement|A combined field used to describe this agreement when associating it with other records" sample="XYZ Vehicle Leasing Contract Guarantee; Expired; 01/01/2004"><CombinedFieldRef name="AgmtTitle"/></CombinedField>
