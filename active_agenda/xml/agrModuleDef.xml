<Module name="Agreements" moduleID="agr" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ModuleDef.xsd" singularRecordName="Agreement" pluralRecordName="Agreements" allowAddRecord="no">
	<ModuleFields>
		<TableField name="AgreementID" type="int" dbFlags="unsigned auto_increment not null" phrase="Record ID|An automatically assigned number used to associate agreements and contracts with related organizations, people, property, and events" sample="1"/>
		<TableField name="RelatedModuleID" type="varchar(5)" validate="noValidation" phrase="Related Module|The name or title of the module which is associated with the agreement" sample="cor"/>
		<ForeignField name="RelatedModule" foreignTable="mod" foreignKey="ModuleID" foreignField="Name" key="RelatedModuleID" joinType="left" phrase="Related Module|The name or title of the module which is associated with the agreement" sample="Corrective Actions"/>
		<TableField name="RelatedRecordID" type="int" validate="noValidation" phrase="Related Record|The unique ID of the related record within the related module" sample="1"/>
		<DynamicForeignField name="RelatedRecordDescription" key="RelatedRecordID" moduleIDField="RelatedModuleID" foreignField="RecordDescription" joinType="left" phrase="Related Record Description|A description of the related record based on a combination of field values assigned from within the related module" sample="Dan Zahlis | 11-14-2005 | Written Warning"/>
		<TableField name="OrganizationID" type="int" defaultValue="defaultorgID" listType="allParticipants" validate="RequireSelection" phrase="Organization|The name of the primary organization associated with the agreement" sample="1"/>
		<ForeignField name="Organization" foreignTable="org" foreignKey="OrganizationID" foreignField="Name" key="OrganizationID" joinType="left" phrase="Organization|The name of the primary organization associated with the agreement" sample="Active Agenda, HQ"/>
		<RemoteField name="PersonAccountableID" defaultValue="defaultorgID" listType="internalParticipants internalNonParticipants externalParticipants externalNonParticipants" validate="RequireSelection" remoteModuleID="acc" remoteModuleIDField="SourceModuleID" remoteRecordIDField="SourceRecordID" remoteField="PersonAccountableID" remoteDescriptorField="AccountabilityDescriptorID" remoteDescriptor="7" phrase="Party Accountable|The name of the person with accountability for the agreement" sample="1"/>
		<ForeignField name="PersonAccountable" foreignTable="ppl" foreignKey="PersonID" foreignField="DisplayName" key="PersonAccountableID" joinType="left" phrase="Party Accountable|The name of the person with accountability for the agreement" sample="Mattias Thorslund"/>
		<TableField name="SecondOrganizationID" type="int" listType="allParticipants" validate="RequireSelection" phrase="Second Organization|The name of the second organization associated with the agreement" sample="1"/>
